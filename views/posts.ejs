<%-include('./parts/head')%>
<table border="1">
    <thead>
        <tr>
            <th>Id</th>
            <th>Item_Name</th>
            <th>Quantity</th>
            <th>Price</th>
            <th>Purchased_on</th>
        </tr>
    </thead>
    <tbody>
    <% posts.forEach((post) => {%>
        <tr>
            <td><%= post.Id%></td>
            <td><%= post.Item_Name%></td>
            <td><%= post.Quantity%></td>
            <td><%= post.Price%></td>
            <td><%= post.Created_on%></td>
        </tr>
    <%})%>
    </tbody>
</table>
<br><br>
<p>Total Cost : <%= total.toString()%></p>
<br><br>
<form action = '/post'>
    <button type="submit">Click here For adding new Item in List.</button>
</form>
<br><br>
<form action = '/delete'>
    <button type="submit">Click here For Delete Item from List.</button>
</form>
<%-include('./parts/tail')%>